{
  "name": "sflogger",
  "version": "0.0.2",
  "description": "A simple log viewer for objects from Salesforce through Heroku Connect/Postgres",
  "author": "Rohit Macherla <rohit.macherla@gmail.com>",
  "private": false,
  "scripts": {
    "start": "node ./bin/start"
  },
  "engines": {
    "node": "6.10.3"
  },
  "dependencies": {
    "body-parser": "~1.8.*",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "jade": "~1.6.0",
    "mocha": "~2.1.0",
    "morgan": "~1.3.0",
    "nforce": "^1.7.0",
    "pg": "4.2.x",
    "socket.io": "^1.3.5"
  },
  "env": {
    "SF_USERNAME": {
      "description": "Your Saleforce.com username"
    },
    "SF_PASSWORD": {
      "description": "Your Saleforce.com password and security token."
    },
    "SF_CLIENT_ID": {
      "description": "Your Connected App's Consumer Key."
    },
    "SF_CLIENT_SECRET": {
      "description": "Your Connected App's Consumer Secret."
    },
    "STREAMING_TOPIC_NAME": {
      "description": "Push topic name for the object you want to log"
    }
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-jshint": "^2.0.4",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1"
  }
}
